<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../Schemas/MorphDef.xsd">
	<Pawnmorph.MorphDef ParentName="CanineMorph">
		<defName>YorkshireTerrierMorph</defName>
		<label>yorkiemorph</label>
		<description>A very yappy voice and an eager temperament denotes a yorkiemorph more than anything. A type of canidmorph, yorkies are too cute to be angry at.</description>
		<race>YorkshireTerrier</race>
		<classification>Canid</classification>
		<categories>
			<li>Pet</li>
			<li>Canid</li>
		</categories>
		<fullTransformation>PawnmorphYorkshireTerrierTF</fullTransformation>
		<partialTransformation>PawnmorphYorkshireTerrierTFPartial</partialTransformation>
		<raceSettings>
			<graphicsSettings>
				<skinColorOverride>(208,168,38)</skinColorOverride>
			</graphicsSettings>
		</raceSettings>
		<transformSettings>
			<transformationMemory>YorkshireTerrierTransformationMemory</transformationMemory>
			<revertedMemory>YorkshireTerrierRevertedMemory</revertedMemory>
		</transformSettings>
	</Pawnmorph.MorphDef>
</Defs>